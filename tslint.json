{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ],
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "no-bitwise": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}